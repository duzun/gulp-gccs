{
  "name": "gulp-gccs",
  "version": "1.0.3",
  "description": "Gulp plugin to compile JS files using Google Closure Compiler Service",
  "reveal": true,
  "main": "dist/index.js",
  "module": "index.js",
  "esnext": "index.js",
  "dependencies": {
    "gccs": "^1.3.2"
  },
  "devDependencies": {
    "chai": "^4.2",
    "event-stream": "^4.0",
    "gulp": "^4.0",
    "gulp-jshint": "^2.1",
    "gulp-rename": "^2.0",
    "gulp-util": "^3.0",
    "mocha": "^10.2.0",
    "verup": "^1.7"
  },
  "scripts": {
    "verup": "verup",
    "test": "mocha --timeout 15000",
    "build": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duzun/gulp-gccs.git"
  },
  "keywords": [
    "gulpplugin",
    "google-closure-compiler-service",
    "google-closure-compiler",
    "closure-compiler",
    "min",
    "compile",
    "js",
    "javascript",
    "build"
  ],
  "jshintConfig": {
    "node": true,
    "esversion": 6,
    "laxbreak": true,
    "laxcomma": true,
    "undef": true,
    "unused": true,
    "sub": true,
    "expr": true,
    "bitwise": false,
    "eqeqeq": false,
    "boss": true,
    "eqnull": true,
    "scripturl": true,
    "-W041": false
  },
  "author": "Dumitru Uzun <contact@duzun.me> (https://DUzun.Me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duzun/gulp-gccs/issues"
  },
  "homepage": "https://github.com/duzun/gulp-gccs#readme",
  "extra": {
    "verup": {
      "files": [
        "package-lock.json",
        "index.js"
      ]
    }
  }
}
